{
    "manifest_version": 3,
    "name": "Ad Skipper",
    "description": "This extension will skip ads on most websites",
    "version": "1.0",
    "action": {
        "default_icon": "assets/fck.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ]
}